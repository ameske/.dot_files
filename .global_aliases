# COMMAND ALIAS LIST
if [[ $( uname ) == 'Darwin' ]]; then
    alias ls='ls -Glh'
    alias la='ls -Glah'
else
    alias ls='ls -lh --color'
    alias la='ls -lah --color'
fi

alias grep='grep --color -n'
